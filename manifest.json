{
  "manifest_version": 2,
  "name": "倒计时",
  "version": "1.0.0",
  "description": "摸鱼人的倒计时插件",
  "browser_action": {
    "default_popup": "index.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": ["storage", "notifications", "http://*/*", "https://*/*"]
}
